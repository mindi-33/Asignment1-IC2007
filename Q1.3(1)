#include <cmath>

bool isInC(int n) {
    int cnt=0;
    for (int i=2;i*i<=n;i++) {
        if(n%i==0) {
            cnt++;
            int exp=0;
            while(n%i==0) {
                n/=i;
                exp++;
                if(exp>1) return false;
            }
        }
    }
    if (n>1) cnt++;
    return cnt==3;
}
