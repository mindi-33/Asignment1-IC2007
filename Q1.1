#include <iostream>
using namespace std;

int main() {
    int countA = 0;
    for (int n = 0; n <= 12000; n++) {
        if ((n % 3 == 0 || n % 3 == 2) && (n % 7 == 1 || n % 7 == 6)) {
            countA++;
        }
    }
    int totalB = 12001;
    int result = totalB - countA;
    cout  <<result<<endl;
    return 0;
}
