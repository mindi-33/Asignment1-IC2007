#include <iostream>
using namespace std;

bool inC(int n) {
    int cnt=0;
    for (int i=2;i*i<= n;i++) {
        if (n%i== 0) {
            cnt++;
            while(n%i==0) {
                n/=i;
                if(n%i==0) return false;
            }
        }
    }
    if (n>1) cnt++;
    return cnt==3;
}

int main() {
    int cnt1=0;
    for(int n=1;n<=12000;n++) {
        if (inC(n)) cnt1++;
    }
    cout<<cnt1<<endl;
    return 0;
}
